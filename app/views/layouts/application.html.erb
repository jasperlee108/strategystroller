<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<title>Strategy Stroller</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta name="description" content="">
		<meta name="author" content="">

		<%= stylesheet_link_tag    "application", :media => "all" %>

		<%= javascript_include_tag "application" %>
		<!-- Only use application for all js files or individual calls for each to prevent duplication
		<%= javascript_include_tag "custom" %>-->
		<!-- Added for cocoon gem -->
		<!--<%= javascript_include_tag :cocoon %>-->
		<!--<link rel="stylesheet" href="http://code.jquery.com/ui/1.10.2/themes/smoothness/jquery-ui.css" />
		<script src="http://code.jquery.com/jquery-1.9.1.js"></script>
		<script src="http://code.jquery.com/ui/1.10.2/jquery-ui.js"></script>
		<script src="/assets/twitter/bootstrap/bootstrap-dropdown.js">-->

		<%= csrf_meta_tags %>

		<!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
		<!--[if lt IE 9]>
		<script src="../assets/js/html5shiv.js"></script>
		<![endif]-->
	</head>

	<body>

		<div class="navbar navbar-inverse navbar-fixed-top">

			<div class="navbar-inner">
				<div class="container">
					<a class="brand" href="/">Strategy Stroller</a>
					<% if signed_in? %>
					<% if current_user.respond_to?('controlling_unit') %>
					<% if current_user.controlling_unit == true %>
					<!-- <div id ="cu_nav">-->
					<ul class = "nav">
						<% if current_page? (cu_review_path) %>
							<li id ="navbar_forms" class="active">
						<% else %>
							<li id ="navbar_forms">
						<% end %>
							<%= link_to (t :forms), cu_review_path %>
						</li>
						<% if current_page? (graph_panel_path) %>
							<li id="navbar_reports" class="active">
						<% else %>
							<li id="navbar_reports">
						<% end %>
							<%= link_to (t :reports), graph_panel_path %>
						</li>
						<% if current_page? (setup_system_path) %>
							<li id= "navbar_setup" class="active">
						<% else %>
							<li id= "navbar_setup">
						<% end %>
							<%= link_to (t :setup), setup_system_path %>
						</li>
						<!--<li id= "navbar_edit_users"><%= link_to (t :edit_users), edit_users_path %></li>-->
					</ul>

					<!--     </div>-->
					<% end %>
					<% end %>

					<div class="btn-group pull-right">
						<a class="btn dropdown-toggle btn-inverse pull-right" data-toggle="dropdown"> User Settings <span class="caret"></span> </a>
						<ul class="dropdown-menu">
							<li>
								<%= render 'devise/menu/login_items' %>
							</li>
							<li>
								<%= render 'devise/menu/registration_items' %>
							</li>
						</ul>
					</div>
					<% end %>

				</div>

			</div>
		</div>

		<div class="container body_container">
			<!-- Display messages For devise. -->
			<!-- <% if notice %>
			<div class="notice alert">
			<button type="button" class="close" data-dismiss="alert">&times;</button>
			<%= notice %>
			</div>

			<% end %>

			<% if alert %>
			<div class="alert">
			<button type="button" class="close" data-dismiss="alert">&times;</button>
			<%= alert %>
			</div>
			<% end %>-->
			<%= yield %>

		</div>
		<!-- /container -->

		<!--  <div class="container">
		<div class="footer">
		<center><%=t :footer_string%></center>
		</div>
		</div>-->
	</body>
</html>

