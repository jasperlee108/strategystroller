<div class="navbar">
  <div class="navbar-inner"><%= tabs %></div></div>
<div class="well" style="margin-top:0px; padding:0px 10px 0px 10px;">
<h4><u>Projects</u></h4>
<table class="table table-hover table-bordered" style="background-color:white;">
  <tr>
    <th>Projects</th>
    <th>Owner</th>
    <th>Status</th>
  </tr>
 <% if @projects.empty? %> 
   <tr><td>&ndash;</td><td>&ndash;</td><td>&ndash;</td>
     </tr>
   <% end %>  
<% @projects.each do |project| %>
  <tr>
  <td> <%= link_to project.short_name , project_check_path(project.id) %></td>
  <td><%= project.user_id %> </td>
  <td><%= project.status %> </td>

  </tr>
<% end %>
</table>
</div>