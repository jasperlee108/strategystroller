<div class="logincontainer" style="background-color: #f5f5f5">
<div class="form-signin">
<h2 class="form-signin-heading">Sign Up</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <%= f.text_field :username, :autofocus => true, :class => "input-block-level", :placeholder => "Username"%>

  <%= f.email_field :email, :class => "input-block-level", :placeholder => "example@example.com"%>

  <%= f.text_field :business_code, :class => "input-block-level", :placeholder => "Buisness Code" %>

  <%= f.password_field :password, :class => "input-block-level", :placeholder => "Password" %>

  <%= f.password_field :password_confirmation,:class => "input-block-level", :placeholder => "Confirm Password" %>

	<div class="row-fluid">
       <%= f.submit "Sign in", :class=> "btn btn-large btn-primary"%>
	</div>
<% end %>

<%= render "devise/shared/links" %>
</div>
</div>